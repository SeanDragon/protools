<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns="http://www.mojo.com/schema/mojo/time-server-config"
            xmlns:xsd="http://www.w3.org/2001/XMLSchema"
            xmlns:beans="http://www.springframework.org/schema/beans"
            targetNamespace="http://www.mojo.com/schema/mojo/time-server-config"
            elementFormDefault="qualified"
            attributeFormDefault="unqualified">

    <xsd:import namespace="http://www.springframework.org/schema/beans"/>

    <xsd:element name="TimeServer">
        <xsd:complexType>
            <xsd:complexContent>
                <xsd:extension base="beans:identifiedType">
                    <xsd:attribute name="timeKey" type="xsd:string" use="required"/>
                    <xsd:attribute name="redisAdapterId" type="xsd:string" use="required"/>
                </xsd:extension>
            </xsd:complexContent>
        </xsd:complexType>
    </xsd:element>

</xsd:schema>